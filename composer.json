    "time": "2016-03-06",
    "keywords": ["array", "geo"],
    "type": "library",
    "authors": [
        {
            "name": "Denis Stokovsky",
            "email": "dstokovsky@gmail.com"
        }
    ],
    "support": {
        "email": "dstokovsky@gmail.com"
    },
    "require": {
        "php": ">=5.5.0",
        "mjaschen/phpgeo": "^1.0"
    },
    "require-dev": {
